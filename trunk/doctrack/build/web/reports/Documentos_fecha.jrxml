	<queryString>
		<![CDATA[select descripcion, fechaentrada, numero_doc, numero_supervision, direccion, nombre
from (select * from documentos where fechaentrada between $P{fechaInicio} and $P{fechaFin}) d
	inner join (select s.id, numero_supervision, idDistrito, direccion, d.nombre from superviciones s inner join distritos d on s.idDistrito = d.id where d.nombre LIKE $P{distrito}) sd on d.idSupervicion = sd.id
order by fechaentrada]]>
	</queryString>
